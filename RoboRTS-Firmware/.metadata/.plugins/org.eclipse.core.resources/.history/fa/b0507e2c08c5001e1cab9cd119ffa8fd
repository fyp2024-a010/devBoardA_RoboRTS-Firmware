/* Includes ------------------------------------------------------------------*/
#include "FreeRTOS.h"
#include "task.h"
#include "main.h"
#include "cmsis_os.h"
#include "init.h"

osThreadId_t imuHandle;
const osThreadAttr_t imu_attributes = {
  .name = "some",
  .stack_size = 128 * 4,
  .priority = (osPriority_t) osPriorityNormal,
};

void app_init(void);


/**
  * @brief  FreeRTOS initialization
  * @param  None
  * @retval None
  */
void app_init(void) {
  imuHandle = osThreadNew(imu_t, NULL, &some_attributes);
}



void imu_t(void *argument);

void imu_t(void *argument)
{
  for(;;)
  {
	  HAL_GPIO_TogglePin(LED_B_GPIO_Port, LED_B_Pin);
    osDelay(1000);
  }
}



